<%- include('../partials/header') %>
<header class="masthead">
    <div class="container">
      <div id="vehicle-forum" class="masthead-subheading">VEHICLE FOURMS</div>
    </div>
    </header>
<table  class="table table-hover table-dark">
    <thead>
        <tr>
            <th>Model</th>
        </tr>
    </thead>
    <tbody>
        <% cars.forEach(function(c) { %>
            <tr>
                <td><%= c.model %></td>
                <td><a class="badge badge-danger" href="/cars/<%= c._id %>">Click here</a></td>
            </tr>
            <% }); %>
    </tbody>
</table>
  
<%- include('../partials/footer') %>
